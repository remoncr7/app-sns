<h1>投稿する</h1>

<%= form_for @post, :url => { action: "create"} do |f| %>
   <%= f.label :タイトル %><br />
   <%= f.text_field :title %><br >
   <%= f.label :画像 %><br />
   <%= f.file_field :img %><br />
   <%= f.label :テキスト %><br />
   <%= f.text_area :text %><br >
   <%= f.label :参考URL %><br />
   <%= f.text_field :url %><br >
   <%= f.submit 'アップロードする' %>
<% end %>

